#include<stdio.h>
 void main()
 {


int i,j;
int a[3][3];

printf("\n\n\t\t\t\t\tEnter the elments for '2'D array.\n");

for(i=0;i<=2;i++)
{
    for(j=0;j<=2;j++)
    {
        scanf("%d",&a[i][j]);
    }
}
printf("\n");
for(i=0;i<=2;i++)
{
    for(j=0;j<=2;j++)
    {
        printf("%d\t",a[i][j]);
    }
    printf("\n");
}

 }
